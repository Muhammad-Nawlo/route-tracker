{
    "name": "muhammad-nawlo/route-tracker",
    "description": "This package to make track of laravel project routers",
    "keywords": [
        "Muhammad-Nawlo",
        "route-tracker"
    ],
    "type": "library",
    "homepage": "https://github.com/muhammad-nawlo/route-tracker",
    "license": "MIT",
    "authors": [
        {
            "name": "Muhammad Nawlo",
            "email": "nawlomuhammadit@gmail.com",
            "role": "Developer"
        }
    ],
    "require": {
        "php": "^8.0",
        "ext-json": "*",
        "illuminate/console": "^10.0 || ^11.0 || ^12.0",
        "illuminate/support": "^10.0 || ^11.0 || ^12.0"
    },
    "require-dev": {
        "laravel/pint": "^1.0",
        "orchestra/testbench": "^10.4",
        "pestphp/pest": "^3.8",
        "spatie/ray": "^1.28"
    },
    "autoload": {
        "psr-4": {
            "MuhammadNawlo\\RouteTracker\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "MuhammadNawlo\\RouteTracker\\Tests\\": "tests"
        }
    },
    "scripts": {
        "test": "vendor/bin/pest",
        "test-coverage": "vendor/bin/pest --coverage",
        "format": "vendor/bin/pint"
    },
    "config": {
        "sort-packages": true,
        "allow-plugins": {
            "pestphp/pest-plugin": true,
            "phpstan/extension-installer": true
        }
    },
    "minimum-stability": "dev",
    "prefer-stable": true,
    "extra": {
        "laravel": {
            "providers": [
                "MuhammadNawlo\\RouteTracker\\RouteTrackerServiceProvider"
            ]
        }
    }
}
